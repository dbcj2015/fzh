    <servlet-name>testSession2</servlet-name>
    <servlet-class>com.bjsxt.session.testSession2</servlet-class>
  </servlet>







  <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>loginPage</servlet-name>
    <url-pattern>/page</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>loginDao</servlet-name>
    <url-pattern>/dao</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>testRedirectServlet</servlet-name>
    <url-pattern>/redirect</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>testCookies</servlet-name>
    <url-pattern>/cookies</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>testSession</servlet-name>
    <url-pattern>/session</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>testSession2</servlet-name>
    <url-pattern>/session2</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
